sqoop job --meta-connect jdbc:hsqldb:hsql://localhost:16000/sqoop --create timeUpdate -- import --connect jdbc:mysql://localhost/cdw_sapp --driver com.mysql.jdbc.Driver --query "select distinct TIMEID, DAY, MONTH, QUARTER, YEAR from (SELECT cast(concat(YEAR,if(month>9,MONTH,CONCAT(0,MONTH)),if(DAY>9,DAY,CONCAT(0,DAY))) as unsigned) as TIMEID, DAY, MONTH, if(MONTH <4,'1st',if(MONTH<7,'2nd',if(MONTH<10,'3rd','4th'))) as QUARTER, YEAR from cdw_sapp_creditcard) as TIMETEMP  WHERE \$CONDITIONS" --fields-terminated-by ',' --target-dir /user/maria_dev/Credit_Card_System/CDW_SAPP_D_TIME/ --incremental append --check-column TIMEID --last-value '0' -m1