 sqoop import\
 --connect jdbc:mysql://localhost/cdw_sapp --driver com.mysql.jdbc.Driver\
 -m1\
 --query "SELECT BRANCH_CODE,
 BRANCH_NAME,
 BRANCH_STREET,
 BRANCH_CITY,
 BRANCH_STATE,
 IFNULL(BRANCH_ZIP,999999) as BRANCH_ZIP,
 concat('(', left(BRANCH_PHONE,3), ')' '-', mid(BRANCH_PHONE,4,3),'-', right(BRANCH_PHONE,4)) AS BRANCH_PHONE,
 LAST_UPDATED
 from cdw_sapp_branch WHERE \$CONDITIONS"\
 --target-dir /user/maria_dev/Credit_Card_System/CDW_SAPP_BRANCH/ --hive-import\
 --hive-database default\
 --hive-table CDW_SAPP_D_BRANCH\
 --create-hive-table\
 --split-by ','\

